<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout"
         xmlns:t="/lib/hudson" xmlns:f="/lib/form" xmlns:cv="/lib/custom">

    <f:entry>
        <f:checkbox field="checked" name="rerunSettingsModels.checked" style="width: 30px;"/>

        <f:textbox field="test" name="rerunSettingsModels.test" value="${instance.test}" style="width: 350px; margin-left: 10px;" />

        <f:entry>
            <label class="attach-previous" style="margin-right: -15px;">Reruns:</label>
            <f:number field="numberOfReruns" name="rerunSettingsModels.numberOfReruns" value="${instance.numberOfReruns}" default="0" style="width: 60px; margin-left: 20px;"/>
        </f:entry>

        <f:entry>
            <label class="attach-previous" style="margin-right: 2px;">Cleanup test:</label>
            <f:textbox field="cleanupTest" name="rerunSettingsModels.cleanupTest" value="${instance.cleanupTest}" style="width: 350px;"/>
        </f:entry>
    </f:entry>
</j:jelly>
